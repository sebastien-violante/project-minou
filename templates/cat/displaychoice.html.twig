{% extends 'base.html.twig' %}

{% block title %}Hello CatController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
   
<h2>Signaler un chat</h2>
<br>
<form action="" method="post">
    <input type="text" name="Ville">
    <p>Vous pouvez affiner votre recherche en précisant jusqu'à trois couleurs</p>
    <input type="text" name="color1" placeholder="première couleur">
    <input type="text" name="color2" placeholder="deuxième couleur">
    <input type="text" name="color3" placeholder="troisième couleur">
    <button type="submit">Validez vos critères de recherche</button>
</form>

</div>

